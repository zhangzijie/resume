<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="jobseekerService" class="com.zhangzj.resume.service.impl.JobseekerServiceImpl">
        <property name="jobseekerDao" ref="jobseekerDao"></property>
    </bean>
    <bean id="companyService" class="com.zhangzj.resume.service.impl.CompanyServiceImpl">
        <property name="companyDao" ref="companyDao"></property>
    </bean>
    <bean id="resumeService" class="com.zhangzj.resume.service.impl.ResumeServiceImpl">
        <property name="resumeDao" ref="resumeDao"></property>
    </bean>
    
</beans>